      "title": "gpuserver1 GPU Power", "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 50},
            {"color": "orange", "value": 100},
            {"color": "red", "value": 200}
          ]},
          "max": 300, "min": 0, "unit": "watt"
        },
        "overrides": []
      },
      "gridPos": {"h": 5, "w": 6, "x": 12, "y": 1}, "id": 203,
      "options": {"colorMode": "value", "graphMode": "area", "orientation": "horizontal", "reduceOptions": {"calcs": ["lastNotNull"]}, "textMode": "auto"},
      "targets": [{"expr": "gpu_power_draw_watts{instance=~\".*gpuserver2.*|.*192.168.1.71.*\"}", "legendFormat": "GPU{{gpu}}"}],
      "title": "gpuserver2 GPU Power", "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 600},
            {"color": "orange", "value": 900},
            {"color": "red", "value": 1200}
          ]},
          "max": 1500, "min": 0, "unit": "watt"
