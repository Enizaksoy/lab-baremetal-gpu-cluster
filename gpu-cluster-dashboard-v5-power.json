        {"expr": "gpu_power_draw_watts{instance=~\".*gpuserver1.*|.*192.168.1.73.*\"}", "legendFormat": "Server1 GPU{{gpu}}"},
        {"expr": "gpu_power_draw_watts{instance=~\".*gpuserver2.*|.*192.168.1.71.*\"}", "legendFormat": "Server2 GPU{{gpu}}"}
      ],
      "title": "GPU Power Draw Over Time (Stacked)", "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {"defaults": {"color": {"mode": "palette-classic"}, "custom": {"drawStyle": "line", "fillOpacity": 10, "lineWidth": 2, "showPoints": "never"}, "max": 1500, "min": 0, "unit": "watt"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}, "id": 206,
      "options": {"legend": {"calcs": ["last", "max", "mean"], "displayMode": "table", "placement": "bottom", "showLegend": true}, "tooltip": {"mode": "multi"}},
      "targets": [
        {"expr": "sum(gpu_power_draw_watts)", "legendFormat": "Total GPU Power"},
        {"expr": "sum(gpu_power_draw_watts) + 400", "legendFormat": "Est. Cluster Total (GPU+Base)"}
      ],
      "title": "Total Power Over Time", "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "continuous-GrYlRd"},
          "custom": {"neutral": 0},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 100},
            {"color": "red", "value": 200}
          ]},
          "max": 300, "min": 0, "unit": "watt"
        },
        "overrides": []
      },
      "gridPos": {"h": 5, "w": 24, "x": 0, "y": 14}, "id": 207,
      "options": {"displayMode": "gradient", "minVizHeight": 10, "minVizWidth": 0, "orientation": "horizontal", "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}, "showUnfilled": true, "text": {}},
      "targets": [{"expr": "gpu_power_draw_watts", "legendFormat": "{{instance}} GPU{{gpu}}"}],
      "title": "GPU Power Gauge (per GPU)", "type": "bargauge"
    },
    {"collapsed": false, "gridPos": {"h": 1, "w": 24, "x": 0, "y": 19}, "id": 100, "title": "GPU Metrics", "type": "row"},
