        "overrides": []
      },
      "gridPos": {"h": 5, "w": 6, "x": 18, "y": 1}, "id": 204,
      "options": {"colorMode": "value", "graphMode": "area", "orientation": "auto", "reduceOptions": {"calcs": ["lastNotNull"]}, "textMode": "auto"},
