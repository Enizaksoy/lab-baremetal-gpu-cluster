        {"matcher": {"id": "byRegexp", "options": ".*GPU0.*"}, "properties": [{"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}}]},
        {"matcher": {"id": "byRegexp", "options": ".*GPU1.*"}, "properties": [{"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}]}
      ]},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}, "id": 205,
