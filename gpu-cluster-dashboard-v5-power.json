{
  "annotations": {"list": []},
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {"collapsed": false, "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}, "id": 200, "title": "Power Monitoring", "type": "row"},
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 150},
            {"color": "orange", "value": 200},
            {"color": "red", "value": 250}
          ]},
          "max": 300, "min": 0, "unit": "watt",
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": {"h": 5, "w": 6, "x": 0, "y": 1}, "id": 201,
      "options": {"colorMode": "value", "graphMode": "area", "orientation": "auto", "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}, "textMode": "auto"},
      "targets": [{"expr": "sum(gpu_power_draw_watts)", "legendFormat": "Total GPU Power"}],
      "title": "Total GPU Power (All Servers)", "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 50},
            {"color": "orange", "value": 100},
            {"color": "red", "value": 200}
          ]},
          "max": 300, "min": 0, "unit": "watt"
